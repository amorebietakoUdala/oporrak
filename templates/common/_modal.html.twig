<div class="modal fade" tabindex="-1" aria-hidden="true" data-entity-target="modal">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<h5 class="modal-title" data-entity-target="modalTitle">{{ modalTitle }}</h5>
				<button type="button" class="close" data-dismiss="modal" aria-label="Close">
					<span aria-hidden="true">&times;</span>
				</button>
			</div>
			<div class="modal-body" data-entity-target="modalBody">
				{{ include (formTemplate,{
                   form: form
                }) }}
			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-secondary" data-dismiss="modal" aria-label="Close">{{'btn.cancel' | trans}}
				</button>
				<button type="button" class="btn btn-primary" data-action="entity#submitForm" data-entity-target="modalSaveButton">
					{{'btn.save' | trans}}
				</button>
			</div>
		</div>
	</div>
</div>
